            "Kompo\\Komposers\\": "src/Komposers",
            "Kompo\\Routing\\": "src/Routing",
            "Kompo\\Utilities\\": "src/Utilities",
            "Kompo\\Tests\\": "tests/"
        }
    },
    "extra": {
