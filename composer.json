{
    "name": "kompo/kompo",
    "description": "Laravel & Vue.js FullStack Components for Rapid Application Development",
    "keywords": [
        "laravel","php","vue","vuejs",
        "component","components",
        "forms","tables","query",
        "framework","cms",
        "button","buttons",
        "modal","modals",
        "link","links"
    ],
    "license": "MIT",
    "require": {
    	"php": ">=7.1.0",
        "laravel/framework": ">=5.6.0",
        "intervention/image": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Kompo\\": "src/Usable",
            "Kompo\\Commands\\": "src/Commands",
            "Kompo\\Core\\": "src/Core",
            "Kompo\\Database\\": "src/Database",
            "Kompo\\Elements\\": "src/Elements",
            "Kompo\\Exceptions\\": "src/Exceptions",
            "Kompo\\Http\\": "src/Http",
            "Kompo\\Interactions\\": "src/Interactions",
            "Kompo\\Komponents\\": "src/Komponents",
            "Kompo\\Models\\": "src/Models",
            "Kompo\\Routing\\": "src/Routing",
            "Kompo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kompo\\KompoServiceProvider"
            ],
            "aliases": {
                "KForm": "Kompo\\Form",
                "KQuery": "Kompo\\Query",
                "KTable": "Kompo\\Table",
                "KMenu": "Kompo\\Menu"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
