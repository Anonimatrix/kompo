{
    "name": "kompophp/kompo",
    "description": "Laravel & Vue.js FullStack Components for Rapid Application Development",
    "keywords": [
        "laravel","php","vue","vuejs",
        "component","components",
        "forms","tables","query",
        "framework","cms",
        "button","buttons",
        "modal","modals",
        "link","links"
    ],
    "license": "MIT",
    "require": {
    	"php": ">=7.1.0",
        "laravel/framework": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Kompo\\": "src/Usable",
            "Kompo\\Commands\\": "src/Commands",
            "Kompo\\Core\\": "src/Core",
            "Kompo\\Elements\\": "src/Elements",
